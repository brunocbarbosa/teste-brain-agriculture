// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model RuralProducer {
  id               String @id @default(uuid())
  cpfOrCnpj        String
  producerName     String
  farmName         String
  city             String
  state            String
  totalArea        String
  agriculturalArea String
  vegetationArea   String
}

model PlantedCrops {
  id              String @id @default(uuid())
  ruralProducerId String
  name            String
}
